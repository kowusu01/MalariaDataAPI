    },
    "LocalPostgres": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CurrentEnvironmentLabel": "Using local Postgres",
        "DataSourceType": "Server"

      },
      "applicationUrl": "https://localhost:7096;http://localhost:5096",
      "dotnetRunMessages": true
    },
    "CloudDb": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CurrentEnvironmentLabel": " Using cloud hosted Postgres",
        "DataSourceType": "cloud"

      },
      "applicationUrl": "https://localhost:7096;http://localhost:5096",
      "dotnetRunMessages": true
    }
  }
