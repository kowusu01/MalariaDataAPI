{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:21670",
      "sslPort": 44331
    }
  },
  "profiles": {
    "InMemoryDb": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CurrentEnvironmentLabel": "(DEV)",
        "DataSourceType": "InMemory"

      },
      "applicationUrl": "https://localhost:7096;http://localhost:5096",
